{
    "name": "website",
    "version": "1.0.0",
    "description": "input source files for personal website",
    "scripts": {
        "build": "npx @11ty/eleventy",
        "serve": "npx @11ty/eleventy --serve",
        "publish": "gh-pages --dist $npm_package_config_output_dir --repo $npm_package_config_prod_repo --branch $npm_package_config_prod_branch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/matthewgrossman/website.git"
    },
    "author": "Matthew Grossman",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/matthewgrossman/website/issues"
    },
    "homepage": "https://github.com/matthewgrossman/website",
    "devDependencies": {
        "@11ty/eleventy": "^0.11.0",
        "gh-pages": "*"
    },
    "config": {
        "prod_repo": "git@github.com:matthewgrossman/matthewgrossman.github.io.git",
        "prod_branch": "master",
        "output_dir": "_site"
    }
}
